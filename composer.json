{
    "name": "barilee/yii2-auth-rbac",
    "description": "RBAC Authentication manager for Yii2 ",
    "keywords": ["yii2", "admin", "auth", "rbac", "authentication", "role based", "administration"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/barilee/yii2-auth-rbac/issues",
        "source": "https://github.com/barilee/yii2-auth-rbac"
    },
    "authors": [
        {
            "name": "Misbahul Munir",
            "email": "misbahuldmunir@gmail.com"
        },
        {
            "name": "Barilee Apenu",
            "email": "barileeapenu@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": "~2.0.7"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "~2.0"
    },
    "suggest": {
        "yiisoft/yii2-bootstrap": "Used when using layout 'left-menu', 'right-menu' or 'top-menu'"
    },
    "autoload": {
        "psr-4": {
            "barilee\\yii2\\auth\\rbac\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-1.0.master": "1.0.x-dev",
            "dev-master": "2.x-dev"
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
